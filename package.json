{
  "name": "rbm-viz",
  "version": "0.1.0",
  "description": "web-based data visualization for risk-based monitoring",
  "module": "src/index.js",
  "main": "index.js",
  "scripts": {
    "build": "esbuild src/main.js --outfile=index.js --bundle --minify --sourcemap",
    "format": "npx prettier --write src",
    "watch": "esbuild src/main.js --outfile=index.js --bundle --minify --sourcemap --watch",
    "local": "live-server ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gilead-BioStats/rbm-viz.git"
  },
