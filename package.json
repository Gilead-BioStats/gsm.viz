{
  "name": "rbm-viz",
  "version": "0.1.0",
  "description": "web-based data visualization for risk-based monitoring",
  "module": "src/index.js",
  "main": "index.js",
  "scripts": {
    "build": "npm run bundle && npm run format",
    "bundle": "esbuild src/main.js --outfile=index.js --bundle --minify --sourcemap",
    "format": "npm run format-src && npm run format-examples",
    "format-src": "npx prettier --single-quote --tab-width=4 --write src",
    "format-examples": "npx prettier --single-quote --tab-width=4 --write examples",
    "watch": "esbuild src/main.js --outfile=index.js --bundle --minify --sourcemap --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gilead-BioStats/rbm-viz.git"
  },
  "keywords": [
    "rbm",
    "risk-based",
    "monitoring",
    "data",
    "visualization"
  ],
  "author": "Gilead Sciences",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gilead-BioStats/rbm-viz/issues"
  },
  "homepage": "https://github.com/Gilead-BioStats/rbm-viz#readme",
  "dependencies": {
    "chart.js": "^3.9.1",
    "chartjs-plugin-annotation": "^2.0.1",
    "chartjs-plugin-datalabels": "^2.1.0",
    "d3": "^7.6.1",
    "esbuild": "^0.15.5",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0"
  }
}
